#There are multiple IDs that needs to be searched in a file whether they are present or not. All the IDs that needs to be searched are kept in sampleIDs.dat file. The file that needs to be searched is searchingFile.dat.

#!/bin/bash
#set -x
echo “START”
while read p;
do
	if $(grep -q “$p” searchFilePath.dat)
	then
		echo “Found: $p”
	#else
		#echo “Found: $p”
	fi
done < sampleIDs.dat
echo “END”
#set +x
